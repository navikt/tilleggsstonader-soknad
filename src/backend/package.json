{
  "name": "tilleggsstonader-soknad-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "ci": "yarn --prefer-offline --frozen-lockfile",
    "build": "tsc --build --verbose",
    "start": "NODE_ENV=production node --import=./build/register.js --es-module-specifier-resolution=node build/server.js",
    "start:dev": "NODE_ENV=development ENV=localhost node --import=./build/register.js --es-module-specifier-resolution=node build/server.js"
  },
  "dependencies": {
    "@navikt/nav-dekoratoren-moduler": "^3.1.3",
    "cookie-parser": "^1.4.7",
    "csp-header": "^6.1.0",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "express-http-proxy": "^2.0.0",
    "html-react-parser": "^5.1.18",
    "http-proxy-middleware": "^3.0.3",
    "jsdom": "^26.0.0",
    "jsonwebtoken": "9.0.2",
    "node-jose": "2.2.0",
    "openid-client": "5.6.5",
    "react": "^19.1.0",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node-jose": "^1.1.10",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2"
  }
}
