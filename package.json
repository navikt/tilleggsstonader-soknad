    "prepare": "husky",
    "lint": "eslint 'src/**/*'",
    "lint:fix": "eslint 'src/**/*' --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix --max-warnings=0 --no-warn-ignored"
    ]
